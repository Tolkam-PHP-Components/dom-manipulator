        "symfony/css-selector": "^5.2",
        "masterminds/html5": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
