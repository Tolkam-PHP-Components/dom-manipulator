{
    "name": "tolkam/dom-manipulator",
    "description": "Eases DOM reading and modification",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Tolkam\\DOM\\Manipulator\\": "src",
            "Tolkam\\DOM\\Manipulator\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
